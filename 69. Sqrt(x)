class Solution:
    def mySqrt(self, x: int) -> int:
        if(x%2==0 or x%1==0):
            //check whether the given number is whole number or not 
            x=sqrt(x)
            x=int(x)//for returning as integer
            return x
        else:
            num=x%1
            x-=num
            //used for remove the point values
            x=sqrt(x)
            x=int(x)//for returning as integer
            return x
        
